{
    "manifest_version": 3,
    "name": "Notifications",
    "version": "0.1.1",
    "description": "Extension to receive notifications from EVMs",
    "icons": {
        "48": "icons/notification_icon.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "Notify",
        "default_popup": "popup.html"
    },
    "permissions": [
        "notifications",
        "activeTab",
        "storage"
    ]
}
